pipeline {
    agent any
    stages {
        stage('version') {
            steps {
                // Execute a PowerShell command to get the version
                powershell 'Get-Host | Select-Object Version'
            }
        }
        stage('hello') {
            steps {
                // Run the Python script using python command
                bat 'python sample.py'
            }
        }
    }
}
